## CREATE BACKGROUNDGENES FROM WP
## GO HERE : https://sparql.wikipathways.org/
## AND MAKE THIS REQUEST : 

        SELECT DISTINCT ?hgncId
            WHERE {
              {
                ?pathway a wp:Pathway ;
                        wp:organismName "Homo sapiens" ;
                        dc:title ?title ;
                        dcterms:identifier ?WPID.
                ?gene a wp:GeneProduct ;
                        dcterms:isPartOf ?pathway ;
                        wp:bdbHgncSymbol ?hgncId .
                }
              UNION
              {
                ?pathway a wp:Pathway ;
                        wp:organismName "Homo sapiens" ;
                        dc:title ?title ;
                        dcterms:identifier ?WPID.
                ?protein a wp:Protein ;
                        dcterms:isPartOf ?pathway ;
                        wp:bdbHgncSymbol ?hgncId .
                }
            }
## SAVE THE FILE INTO bgGenesWP_DATE.csv
sed -i 's/"//g' bgGenesWP_DATE.csv 
sed -i 's/.*symbol\///g' bgGenesWP_DATE.csv
sed -i '1d' bgGenesWP_DATE.csv
sort bgGenesWP_DATE.csv > bgGenesWP_DATE_expected.csv
